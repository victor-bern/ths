<MudDialog>
    <DialogActions>
        <MudButton OnClick="Cancel">Não</MudButton>
        <MudButton Color="Color.Error" OnClick="Submit">Sim</MudButton>
    </DialogActions>
</MudDialog>
@code {
    [CascadingParameter] MudDialogInstance MudDialog { get; set; }

    void Submit() => MudDialog.Close(DialogResult.Ok(true));
    void Cancel() => MudDialog.Cancel();
}